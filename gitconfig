; check if file changed between prev head, decide if to run some command: git diff --name-only ORIG_HEAD -- filename
[user]
  name = Grant Welch
  email = (none)
[core]
  editor = /usr/bin/env vim
[push]
  default = upstream
[color]
  ui = auto
[color "decorate"]
  branch = bold blue
  remoteBranch = dim red
  tag = ul dim cyan
  stash = reverse bold green
  HEAD = ul green
[color "branch"]
  current = reverse bold yellow
  local = yellow
  remote = dim red
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "grep"]
  filename = yellow bold
  linenumber = blue
[status]
  showUntrackedFiles = no
  short = true
  branch = true
[diff]
  tool = vimdiff
	algorithm = patience
[difftool]
  prompt = false
[merge]
  conflictstyle = diff3
[rerere]
  enabled = true
[alias]
  amend = commit -a --amend --no-edit --reset-author
  reword = commit -a --amend --reset-author
  br = branch
  ci = commit
  co = checkout
  st = status
  stat = status -s -uno
  stl = status --long
  up = pull --rebase
  update = pull --rebase
  gg = grep -EIin --break --heading
  type = cat-file -t
  dump = cat-file -p
  last = cat-file -p HEAD
  logs = log --name-only
  a = "! git alog -20"
  alog = log --graph --pretty=format:\"%C(auto)%h -%d %s %C(green)(%cd) %C(bold blue)<%an>\" --abbrev-commit --date=relative --all
  p = "! git plog -20"
  plog = log --graph --pretty=format:\"%C(auto)%h -%d %s %C(green)(%cd) %C(bold blue)<%an>\" --abbrev-commit --date=relative
  u = "! git ulog -20"
  ulog = log --graph --pretty=format:\"%C(auto)%h -%d %s %C(green)(%cd) %C(bold blue)<%an>\" --abbrev-commit --date=relative HEAD @{u}
  bleach = clean -fdx
  dryclean = clean -ndx
  uncommit = reset HEAD^
  undo = reset --hard --
  unstage = reset HEAD --
  b = branch -vv
  r = remote -v
  ll = log --stat --abbrev-commit
  latest = "! git for-each-ref --sort='-authordate:iso8601' --format=' %(authordate:relative)%09%(refname:short)' refs/remotes/origin"
  hs = "!git shortlog -s -n --no-merges"
  wc = "!f(){ git diff --stat -w $(git merge-base ${2:-HEAD} ${1:-master})..${2:-HEAD}; }; f"
  wcd = "!f(){ git diff $(git merge-base ${2:-HEAD} ${1:-master})..${2:-HEAD}; }; f"
[pull]
  rebase = true
[fetch]
	prune = true
